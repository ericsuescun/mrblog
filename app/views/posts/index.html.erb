<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1>Listado de Post del Super Blog</h1>
      <table class="table">
        <tr>
          <th>N</th>
          <th>Titulo</th>
          <th>Post</th>
        </tr>
        <% @posts.each do |post| %>
          <tr>
            <td><%= post.id %></td>
            <td><%= post.title %></td>
            <td><%= post.content %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>


